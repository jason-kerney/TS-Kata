{
    "branchPrefix": {
        "separator": "",
        "validator": "",
        "prefixes": {}
    },
    "commitPrefix": {
        "separator": " ",
        "validator": "^.{1,45}$",
        "prefixes": {
            "***": "Does not compile -- intermediate step",
            "R": "Refactored on passing tests",
            "R!!": "Refactored on non-passing tests",
            "F": "Added new feature test first",
            "F!!": "Added new feature without tests",
            "*": "Other"
        }
    },
    "verboseMode": false,
    "aliases": [
        {
            "name": "undo",
            "command": "cls; git clean -xfd && git checkout .",
            "description": "resets branch to last checked in state"
        },
        {
            "name": "push",
            "command": "cls; gittey commit && git push",
            "description": "performs a git push"
        },
        {
            "name": "test",
            "command": "cls; npm test && gittey commit",
            "description": "runs tests and commits them if the tests pass"
        },
        {
            "name": "docs",
            "command": "cls; booklisp ./docs/_main.md ./readme.md",
            "description": "if booklisp is installed as a global package then this will regenerate the readme"
        }
    ],
    "collaborators": []
}