{
    "branchPrefix": {
        "separator": "",
        "validator": "",
        "prefixes": {}
    },
    "commitPrefix": {
        "separator": " ",
        "validator": "^.{1,45}$",
        "prefixes": {
            "***": "Does not compile -- intermediate step",
            "R": "Refactored on passing tests",
            "R!!": "Refactored on non-passing tests",
            "F": "Added new feature test first",
            "F!!": "Added new feature without tests",
            "*": "Other"
        }
    },
    "verboseMode": false,
    "aliases": [
        {
            "name": "undo",
            "command": "cls; git clean -xfd && git checkout .",
            "description": "resets branch to last checked in state"
        },
        {
            "name": "test",
            "command": "cls; ts-node node_modules/jasmine/bin/jasmine JASMINE_CONFIG_PATH=./jasmine.json && gittey commit",
            "description": "runs tests and commits on success"
        },
        {
            "name": "push",
            "command": "cls; gittey commit && git push"
        }
    ],
    "collaborators": []
}